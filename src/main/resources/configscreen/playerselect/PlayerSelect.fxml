<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<Label fx:id="playerNumberLabel" contentDisplay="BOTTOM" text="Player #" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="configscreen.playerselect.PlayerSelectPresenter">
   <graphic>
      <HBox>
         <children>
            <Label contentDisplay="RIGHT" text="Name">
               <graphic>
                  <TextField fx:id="name" />
               </graphic>
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
            <Label contentDisplay="RIGHT" text="Race">
               <graphic>
                  <ChoiceBox fx:id="race" prefWidth="150.0" />
               </graphic>
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
            <Label contentDisplay="RIGHT" text="Color">
               <graphic>
                  <ChoiceBox fx:id="color" prefWidth="150.0" />
               </graphic>
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </graphic>
   <padding>
      <Insets bottom="5.0" top="5.0" />
   </padding>
</Label>
